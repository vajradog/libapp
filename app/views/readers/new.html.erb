<%= form_for @reader do |form|%>
<%= form.label :reader_email%>
<%= form.text_field :reader_email%>
<%= form.label :reader_password%>
<%= form.text_field :reader_password%>
<%= form.label :reader_password_confirmation%>
<%= form.text_field :reader_password_confirmation%>
<%=form.submit "Register" %>
<%end %>